{
  "expo": {
    "name": "triphub",
    "slug": "triphub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/triphublogofinal.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/triphublogofinal.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.guillaume.suchgmailcom.triphub"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": [
          "WRITE_EXTERNAL_STORAGE",
          "READ_EXTERNAL_STORAGE",
          "READ_MEDIA_STORAGE",
          "MANAGE_DOCUMENTS",
          "ACCESS_FINE_LOCATION",
          "ACCESS_COARSE_LOCATION"
        ]
      },
      "package": "com.guillaume.suchgmailcom.triphub"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "21fc523c-a142-4867-88a5-38503f02547d"
      }
    }
  }
}
